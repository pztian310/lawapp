<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:pxxy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activitys.ApplyLawyerActivity">

    <com.pxxy.lawconsult.view.CustomTitleView
        android:id="@+id/custom_case_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title_background="@color/theme_color"
        app:title_center_textView_text="案例收藏"
        app:title_center_textView_textColor="@color/white"
        app:title_center_textView_textSize="20sp"
        app:title_leftTv_textColor="@color/white"
        app:title_leftButton_resource="@mipmap/terms_servicebreak" />
        <ImageView
            android:id="@+id/iv_case_image"
            android:layout_marginTop="30dp"
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:visibility="gone"
            android:background="@drawable/evaluate2"/>
    <ListView
        android:id="@+id/lv_case"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

    </ListView>
</LinearLayout>